package com.itwill.loop12;

import java.util.Random;

public class LoopMain12 {

	public static void main(String[] args) {
		// 교재 연습문제 3번
		Random r = new Random();

		int x;
		
		int y;

		do {
			
			x = r.nextInt(1,7);
			
			y = r.nextInt(1,7);
			
				System.out.printf("눈 1 = %d \n 눈 2 = %d ", x, y);
				System.out.println("----------------");

		} while(x + y != 5);
		
		System.out.println(x+y);
		
		// 교재 연습문제 4번
		

	}

}
