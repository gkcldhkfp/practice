package com.itwill.loop07;

public class Loopsub02 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for (;;) {
			System.out.println("---------------------");
			System.out.println("1.예금 | 2.출금 | 3.잔고 | 4.종료");
			System.out.println("---------------------");
			System.out.print("선택> ");
		
		
		
		
		
		
		}

	}

}
