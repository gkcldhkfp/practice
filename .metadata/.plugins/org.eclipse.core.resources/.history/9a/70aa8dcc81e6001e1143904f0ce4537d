package com.itwill.loop12;

import java.util.Random;

public class LoopMain12 {

	public static void main(String[] args) {
		// 교재 연습문제 3번
		Random r = new Random();

		int x = r.nextInt(1,7);
		
		int y = r.nextInt(1,7);

		do {

		System.out.printf("눈 1 = %d\n 눈 2 = %d\n", x, y);

		} while(x + y != 5);
		
		System.out.println(x+y);
		
		// 교재 연습문제 4번
		

	}

}
